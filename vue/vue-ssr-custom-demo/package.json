{
  "name": "vue-ssr-custom-demo",
  "version": "1.0.0",
  "description": "服务器端渲染(Server-side rendering, SSR)",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server",
    "dev": "node server.js",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.conf.js --color --progress",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.conf.js --color --progress",
    "test": "node build/test.js"
  },
